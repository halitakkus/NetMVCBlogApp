@model IEnumerable<NetMVCBlogApp.Entity.Category>

<div class="categories">
    <h2 class="categories-header">
        <span>
            Categories
        </span>
    </h2>
    <ul class="category-list">
        @foreach (var category in Model)
        {
            <li><a href="#">@category.Name</a></li>
        }
    </ul>
</div>
